import UIKit

typealias Emoji = Character
typealias CarModel = (model: String, isAvailable: Bool?, carPrice: Int)

// CarPark
let camry: CarModel = ("Toyota Camry", false, 45)
let passat: CarModel = ("Volkswagen Passat", true, 35)
let prius: CarModel = ("Toyota Prius ", true, 50)
let mustang: CarModel? = ("Ford Mustang", true, 85)

// Countries and travel expenses
let netherlandsDict = [
    "Netherlands": ["Tickets": 250, "Apartaments": 50]
]
let franceDict = [
    "France" : ["Tickets": 175, "Apartaments": 35]
]
let newYorkDict = [
    "NewYork" : ["Tickets": 333, "Apartaments": 450]
]
let japanDict = [
    "Japan" : ["Tickets": 250, "Apartaments": 50]
]
let northKoreaDict = [
    "northKorea" : ["Tickets": 250, "Apartaments": 35]
]

// Countries local vehiecle
let netherlandsCountryCar = camry.isAvailable ?? false ? camry : passat
let franceCountryCar = mustang
let newYorkCountryCar: CarModel? = prius
let japanCountryCar: CarModel? = nil
let northKoreaCountryCar: CarModel? = nil

// Emojies
let croissant: Emoji = "ü•ê"
let excitedMood: Emoji = "ü§†"
let taxi: Emoji = "üöï"

// Other expenses
let croissantPrice = 2
let chocolateBar = 2.50
let cola = 4
let taxiInNyc = 100
let subwayNyc = 10

var tickets = [Int]()
var apartaments = [Int]()
var vehicle = [Int]()
var picturesTaken = [Int]()
var totaleOthers = [Double]()

// Total expanses
var totalTickets = 0
var totalApartaments = 0
var totalVehicleExpanses = 0
var totalPictureTaken = 0
var totalOther: Double = 0
var summary = 0.0

print("–ü—Ä–∏–≤–µ—Ç –¥—Ä—É–∂–∏—â–µ! –°–µ–≥–æ–¥–Ω—è –Ø —Ä–µ—à–∏–ª —É–µ—Ö–∞—Ç—å —Å –ü–æ–ª—å—à–∏ –≤ –¥–æ–ª–≥–æ–∂–¥–∞–Ω–Ω–æ–µ –ø—É—Ç–µ—à–µ—Å—Ç–≤–∏–µ!\(excitedMood)")
print("–ü–æ —Å–ø–∏—Å–∫—É –ø–µ—Ä–≤–æ–π —Å—Ç—Ä–∞–Ω–æ–π –±—É–¥–µ—Ç - \(netherlandsDict.keys.first ?? "No info")")
print("–î–∞–≤–∞–π —Å–¥–µ–ª–∞–µ–º —Ñ–æ—Ç–æ –º–æ–∏—Ö —á–µ–º–∞–¥–∞–Ω–æ–≤")
picturesTaken.append(1)

print("""
–ø–µ—Ä–µ–ª—ë—Ç –≤ –æ–¥–Ω—É —Å—Ç–æ—Ä–æ–Ω—É –º–Ω–µ –≤—ã–π–¥–µ—Ç \(netherlandsDict["Netherlands"]?["Tickets"] ?? 0)$
–∫–∞–∫ –∂–µ –Ω–µ —Å–¥–µ–ª–∞—Ç—å —Ñ–æ—Ç–∫—É –≤ —Å—Ç–æ—Ä–∏—Å –±–∏–ª–µ—Ç–æ–≤ –Ω–∞ —Ñ–æ–Ω–µ –∞–µ—Ä–æ–ø–æ—Ä—Ç–∞
–î–∞–ª–µ–µ –Ø —É–∂–µ –∑–∞—é—Ä–æ–Ω–∏—Ä–æ–≤–∞–ª –æ—Ç–ª–∏—á–Ω—É—é –∏ —É—é–Ω—É—é—Ç –∫–≤–∞—Ä—Ç–∏—Ä–∫—É –Ω–∞ air b&b –Ω–∞ 7 –¥–Ω–µ–π
–ü—Ä–æ—Å—Ç–æ –ø–æ—Å–º–æ—Ç—Ä–∏ –Ω–∞ —ç—Ç–æ—Ç –∫–∞–º–∏–Ω!!!!
–Ø —Å–ª—ã—à–∞–ª, —á—Ç–æ –≤—Å—é –≥–æ–ª–∞–Ω–¥–∏—é –º–æ–∂–Ω–æ –æ–±—å–µ–∑–¥–∏—Ç—å –∑–∞ –Ω–µ–¥–µ–ª—é, –∏ –¥–ª—è —ç—Ç–æ–≥–æ —è —Å–Ω–∏–º—É –ø—Ä–æ—Å—Ç–µ–Ω—å–∫—É—é –º–∞—à–∏–Ω–∫—É \(netherlandsCountryCar) –Ω–∞ –Ω–µ–¥–µ–ª—é –∑–∞ \(netherlandsCountryCar.carPrice * 7)$ –Ω–∞ 7 –¥–Ω–µ–π
–£ –Ω–∞—Å –±—É–¥–µ—Ç –≤—Ä–µ–º—è —Å–¥–µ–ª–∞—Ç—å –µ—â–µ –ø–∞—Ä—É —Ñ–æ—Ç–æ—á–µ–∫
""")
apartaments.append((netherlandsDict["Netherlands"]?["Apartaments"] ?? 0) * 7)
picturesTaken.append(6)
tickets.append(netherlandsDict["Netherlands"]?["Tickets"] ?? 0)
vehicle.append(netherlandsCountryCar.carPrice * 7)
print(vehicle)

print("—Å–∞–º–æ–µ –≤—Ä–µ–º—è –ø–æ—Å–µ—Ç–∏—Ç—å –≤—Ç–æ—Ä—É—é –Ω–∞ —Å–ø–∏—Å–∫–µ —Å—Ç—Ä–∞–Ω—É - \(franceDict.keys.first ?? "No info")")
print("–±–∏–ª–µ—Ç—ã —É–¥–∞–ª–æ—Å—å —Å–ª–æ–≤–∏—Ç—å –Ω–∞ —Å–∫–∏–¥–∫–µ –∑–∞ \(franceDict["France"]?["Tickets"] ?? 0)$ .")
tickets.append(franceDict["France"]?["Tickets"] ?? 0)
if let car = franceCountryCar {
        print("–Ø –±—É–¥—É –ø—Ä–∏–º–µ—Ä–Ω–æ 2 –¥–Ω—è, –ø–æ—ç—Ç–æ–º—É —Å–Ω—è–ª —É–∫—Ä–æ–º–Ω–æ–µ –º–µ—Å—Ç–µ—á–∫–æ —É –ï–π—Ñ–µ–ª–µ–≤–æ–π –ë–∞—à–Ω–∏ —Å –º–∞—à–∏–Ω–æ–π \(car.model) \(car.model == "Ford Mustang" ? "–±–µ–∑ –∫—Ä—ã—à–∏" : "")")
        
        totalApartaments += franceDict["France"]?["Apartaments"] ?? 0
        totalVehicleExpanses += (car.carPrice * 2)
} else {
    print("–Ø –±—É–¥—É –ø—Ä–∏–º–µ—Ä–Ω–æ 2 –¥–Ω—è, –ø–æ—ç—Ç–æ–º—É —Å–Ω—è–ª —É–∫—Ä–æ–º–Ω–æ–µ –º–µ—Å—Ç–µ—á–∫–æ —É –ï–π—Ñ–µ–ª–µ–≤–æ–π –ë–∞—à–Ω–∏, –¥–µ—Ä–∂–∏ –ø–∞—Ä—É —Ñ–æ—Ç–æ–∫)")
    totalApartaments += franceDict["France"]?["Apartaments"] ?? 0
    totalPictureTaken += 2
}
print("–ó–¥–µ—Å—å –±–µ–∑ –∫—Ä—É–∞—Å–∞–Ω–∞ \(croissant) –Ω–∏–∫–∞–∫, –∫—É–ø–∏–º –∏ –µ–¥—É –≤ –ê–µ—Ä–æ–ø–æ—Ä—Ç")
totaleOthers.append(Double(croissantPrice))
tickets.append(franceDict["France"]?["Tickets"] ?? 0)
picturesTaken.append(1)

print("–ò –≤–æ—Ç –¥–æ–ª–≥–æ–∂–¥–∞–Ω—ã–π \(newYorkDict.keys.first ?? "No info")!, –∏ –∑–¥–µ—Å—å —è –±–µ—Ä—É —Å–∞–º—É—é –≤–∏–¥–æ–≤—É—é –∫–≤–∞—Ä—Ç–∏—Ä–∫—É –Ω–∞ air b&b –∑–∞ —Ü–µ–ª—ã—Ö \(newYorkDict["NewYork"]?["Apartaments"] ?? 0)$ –≤ —Å—É—Ç–∫–∏!")
print("–í–æ—Å–ø–æ–ª—å–∑—É—é—Å—å –ø–ª–∞—Ç–Ω—ã–º –±–∞—Ä–æ–º –∏ –≤–æ–∑—å–º—É –∫–æ–ª—É —Å —à–æ–∫–æ–ª–∞–¥–∫–æ–π")
print("–ë–ª–∞–≥–æ —è –∑–¥–µ—Å—å –±—É–¥—É 1 –¥–µ–Ω—å, –ø–æ—ç—Ç–æ–º—É –æ—Ç–¥–æ—Ö–Ω—É—Ç—å —Ä–µ—à–∏–ª –Ω–∞ –ø–æ–ª–Ω—É—é!)")
apartaments.append(newYorkDict["NewYork"]?["Apartaments"] ?? 0)
picturesTaken.append(2)
totaleOthers.append((chocolateBar + Double(cola)))

if let car = newYorkCountryCar {
    print("–ê –≤–æ—Ç –∏ —Ç–∞–∫—Å–∏ \(taxi)!")
    vehicle.append(taxiInNyc)
} else {
    print("–ó–¥–µ—Å—å —Ç–∞–∫–∏–µ –ø—Ä–æ–±–∫–∏ \(taxi), —á—Ç–æ –ª—É—á—à–µ —è –≤–æ—Å–ø–æ–ª—å–∑—É—é –º–µ—Ç—Ä–æ")
    vehicle.append(subwayNyc)
    picturesTaken.append(1)
}

print("–°—Ä–∞–∑—É –ø–æ—Å–ª–µ \(newYorkDict.keys.first ?? "No info")'a —è —Å—Ä–∞–∑—É –∂–µ –Ω–∞–ø—Ä–∞–≤–ª—é—Å—å –≤ \(japanDict.keys.first ?? "No info")")
print("–î–∞–π –ë–æ–≥ –º–Ω–µ –æ–¥–æ–±—Ä—è—Ç –≤–∏–∑—É!")
print("–ö–≤–∞—Ä—Ç–∏—Ä—É –≤–æ–∑—å–º—É –Ω–∞ 3 –¥–Ω—è, –±–µ–∑ –∞–≤—Ç–æ")
tickets.append(japanDict["Japan"]?["Tickets"] ?? 0)
apartaments.append((japanDict["Japan"]?["Apartaments"] ?? 0) * 3)
picturesTaken.append(1)

print("–í –∫—Ä–∞–π–Ω–µ–º —Å–ª—É—á–∞–µ —è –Ω–∞–ø—Ä–∞–≤–ª—é—Å—å –≤ \(northKoreaDict.keys.first ?? "No info")")
print("–∏ –∫—Ä–∞–π–Ω–∏–π –¥–µ–Ω—å —è –ø—Ä–æ–≤–µ–¥—É –∑–¥–µ—Å—å")
tickets.append(northKoreaDict["northKorea"]?["Tickets"] ?? 0)
apartaments.append(northKoreaDict["northKorea"]?["Apartaments"] ?? 0)
totalTickets = tickets.reduce(0, { $0 + $1 })
totalApartaments = apartaments.reduce(0, { $0 + $1 })
totalPictureTaken = picturesTaken.reduce(0, { $0 + $1 })
totalOther = totaleOthers.reduce(0, { $0 + $1 })
totalVehicleExpanses = vehicle.reduce(0, { $0 + $1 })
summary = Double(totalTickets) + Double(totalApartaments) + Double(totalVehicleExpanses) + totalOther

print("""
      –ö–∞–∫ –∏—Ç–æ–≥ –≤—Å—ë –ø—É—Ç–µ—à–µ—Å—Ç–≤–∏–µ –º–Ω–µ –æ–±–æ—à–ª–æ—Å—å –≤ \(summary)$
      –ù–∞ –±–∏–ª–µ—Ç—ã —É –º–µ–Ω—å —É—à–ª–æ –ø–æ—Ä—è–¥–∫–∞ \(totalTickets)$
      –ù–∞ —Å—ä—ë–º –∂–∏–ª—å—è –µ—â—ë + \(totalApartaments)$
      –ê –≤–æ—Ç –Ω–∞ –∞—Ä–µ–Ω–¥—É –∞–≤—Ç–æ–º–æ–±–∏–ª–µ–π - \(totalVehicleExpanses)$
      –ú–µ–Ω—å—à–µ –≤—Å–µ–≥–æ –∑–∞–Ω—è–ª–∏ —Ä–∞—Å—Å—Ç—Ä–∞—Ç—ã –Ω–∞ —Å–ø–æ–Ω—Ç–∞–Ω–Ω—ã–µ —Å–Ω—ç–∫–∏ - \(totalOther)$
      –∏ —è —É—Å–ø–µ–ª —Å–¥–µ–ª–∞—Ç—å \(totalPictureTaken) —Ñ–æ—Ç–æ–∫!!
      """)



