import UIKit

typealias Emoji = Character

// Netherlands
let netherlandsCountry = "Netherlands"
let netherlandsTickets = 250     // per flight
let netherlandsApartaments = 50  // per night
let netherlandsCarPrice = 45     // per day

// France
let franceCountry = "France"
let franceTickets = 175
let franceApartaments = 35
let franceCarPrice = 55
let croissantPrice = 2          // per 1 piece

// New York
let newYorkCountry = "New York"
let newYorkTickets = 333
let newYorkApartaments = 450
let newYorkCountryCarPrice = 120

// Japan
let japanCountry = "Japan"
let japanTickets = 250
let japanApartaments = 50
let japanCarPrice = 75

// North Korea
let northKoreaCountry = "North Korea"
let northKoreaTickets = 250
let northKoreaApartaments = 50
let northKoreaCarPrice = 75

// Emojies
let croissant: Emoji = "ü•ê"
let excitedMood: Emoji = "ü§†"
let taxi: Emoji = "üöï"

// Total expanses
var totalTickets = 0
var totalApartaments = 0
var totalVehicleExpanses = 0
var totalPictureTaken = 0
var totalOther: Double = 0

print("–ü—Ä–∏–≤–µ—Ç –¥—Ä—É–∂–∏—â–µ! –°–µ–≥–æ–¥–Ω—è –Ø —Ä–µ—à–∏–ª —É–µ—Ö–∞—Ç—å —Å –ü–æ–ª—å—à–∏ –≤ –¥–æ–ª–≥–æ–∂–¥–∞–Ω–Ω–æ–µ –ø—É—Ç–µ—à–µ—Å—Ç–≤–∏–µ!\(excitedMood)")
print("–ü–æ —Å–ø–∏—Å–∫—É –ø–µ—Ä–≤–æ–π —Å—Ç—Ä–∞–Ω–æ–π –±—É–¥–µ—Ç - \(netherlandsCountry)")
print("–î–∞–≤–∞–π —Å–¥–µ–ª–∞–µ–º —Ñ–æ—Ç–æ –º–æ–∏—Ö —á–µ–º–∞–¥–∞–Ω–æ–≤")
totalPictureTaken += 1

print("""
–ø–µ—Ä–µ–ª—ë—Ç –≤ –æ–¥–Ω—É —Å—Ç–æ—Ä–æ–Ω—É –º–Ω–µ –≤—ã–π–¥–µ—Ç \(netherlandsTickets)$
–∫–∞–∫ –∂–µ –Ω–µ —Å–¥–µ–ª–∞—Ç—å —Ñ–æ—Ç–∫—É –≤ —Å—Ç–æ—Ä–∏—Å –±–∏–ª–µ—Ç–æ–≤ –Ω–∞ —Ñ–æ–Ω–µ –∞–µ—Ä–æ–ø–æ—Ä—Ç–∞
–î–∞–ª–µ–µ –Ø —É–∂–µ –∑–∞—é—Ä–æ–Ω–∏—Ä–æ–≤–∞–ª –æ—Ç–ª–∏—á–Ω—É—é –∏ —É—é–Ω—É—é—Ç –∫–≤–∞—Ä—Ç–∏—Ä–∫—É –Ω–∞ air b&b –Ω–∞ 7 –¥–Ω–µ–π
–ü—Ä–æ—Å—Ç–æ –ø–æ—Å–º–æ—Ç—Ä–∏ –Ω–∞ —ç—Ç–æ—Ç –∫–∞–º–∏–Ω!!!!
–Ø —Å–ª—ã—à–∞–ª, —á—Ç–æ –≤—Å—é –≥–æ–ª–∞–Ω–¥–∏—é –º–æ–∂–Ω–æ –æ–±—å–µ–∑–¥–∏—Ç—å –∑–∞ –Ω–µ–¥–µ–ª—é, –∏ –¥–ª—è —ç—Ç–æ–≥–æ —è —Å–Ω–∏–º—É –ø—Ä–æ—Å—Ç–µ–Ω—å–∫—É—é –º–∞—à–∏–Ω–∫—É –Ω–∞ –Ω–µ–¥–µ–ª—é –∑–∞ \(netherlandsCarPrice)$ –Ω–∞ 7 –¥–Ω–µ–π
–£ –Ω–∞—Å –±—É–¥–µ—Ç –≤—Ä–µ–º—è —Å–¥–µ–ª–∞—Ç—å –µ—â–µ –ø–∞—Ä—É —Ñ–æ—Ç–æ—á–µ–∫
""")
totalApartaments += (netherlandsApartaments * 7)
totalPictureTaken += 1
totalPictureTaken += 1
totalTickets += netherlandsTickets
totalPictureTaken += 2
totalVehicleExpanses += (netherlandsCarPrice * 7)

print("—Å–∞–º–æ–µ –≤—Ä–µ–º—è –ø–æ—Å–µ—Ç–∏—Ç—å –≤—Ç–æ—Ä—É—é –Ω–∞ —Å–ø–∏—Å–∫–µ —Å—Ç—Ä–∞–Ω—É - \(franceCountry)")
print("–±–∏–ª–µ—Ç—ã —É–¥–∞–ª–æ—Å—å —Å–ª–æ–≤–∏—Ç—å –Ω–∞ —Å–∫–∏–¥–∫–µ –∑–∞ \(franceTickets)$ .")
totalTickets += franceTickets
print("–Ø –±—É–¥—É –ø—Ä–∏–º–µ—Ä–Ω–æ 2 –¥–Ω—è, –ø–æ—ç—Ç–æ–º—É —Å–Ω—è–ª —É–∫—Ä–æ–º–Ω–æ–µ –º–µ—Å—Ç–µ—á–∫–æ —É –ï–π—Ñ–µ–ª–µ–≤–æ–π –ë–∞—à–Ω–∏ —Å –º–∞—à–∏–Ω–æ–π –±–µ–∑ –∫—Ä—ã—à–∏)")
totalVehicleExpanses += (franceCarPrice * 2)
print("–ó–¥–µ—Å—å –±–µ–∑ –∫—Ä—É–∞—Å–∞–Ω–∞ \(croissant) –Ω–∏–∫–∞–∫, –∫—É–ø–∏–º –∏ –µ–¥—É –≤ –ê–µ—Ä–æ–ø–æ—Ä—Ç")
totalOther += Double(croissantPrice)
totalTickets += newYorkTickets
totalPictureTaken += 1

print("–ò –≤–æ—Ç –¥–æ–ª–≥–æ–∂–¥–∞–Ω—ã–π \(newYorkCountry)!, –∏ –∑–¥–µ—Å—å —è –±–µ—Ä—É —Å–∞–º—É—é –≤–∏–¥–æ–≤—É—é –∫–≤–∞—Ä—Ç–∏—Ä–∫—É –Ω–∞ air b&b –∑–∞ —Ü–µ–ª—ã—Ö \(newYorkApartaments)$ –≤ —Å—É—Ç–∫–∏!")
totalPictureTaken += 1
print("–í–æ—Å–ø–æ–ª—å–∑—É—é—Å—å –ø–ª–∞—Ç–Ω—ã–º –±–∞—Ä–æ–º –∏ –≤–æ–∑—å–º—É –∫–æ–ª—É —Å —à–æ–∫–æ–ª–∞–¥–∫–æ–π")
totalOther += (2.50 + Double(4))
totalPictureTaken += 1
print("–ë–ª–∞–≥–æ —è –∑–¥–µ—Å—å –±—É–¥—É 1 –¥–µ–Ω—å, –ø–æ—ç—Ç–æ–º—É –æ—Ç–¥–æ—Ö–Ω—É—Ç—å —Ä–µ—à–∏–ª –Ω–∞ –ø–æ–ª–Ω—É—é!)")
totalApartaments += newYorkApartaments
print("–ó–¥–µ—Å—å —Ç–∞–∫–∏–µ –ø—Ä–æ–±–∫–∏ \(taxi), —á—Ç–æ –ª—É—á—à–µ —è –≤–æ—Å–ø–æ–ª—å–∑—É—é –º–µ—Ç—Ä–æ")
totalVehicleExpanses += 10
totalPictureTaken += 1

print("–°—Ä–∞–∑—É –ø–æ—Å–ª–µ \(newYorkCountry)'a —è —Å—Ä–∞–∑—É –∂–µ –Ω–∞–ø—Ä–∞–≤–ª—é—Å—å –≤ \(japanCountry)")
totalTickets += japanTickets
print("–î–∞–π –ë–æ–≥ –º–Ω–µ –æ–¥–æ–±—Ä—è—Ç –≤–∏–∑—É!")
print("–ö–≤–∞—Ä—Ç–∏—Ä—É –≤–æ–∑—å–º—É –Ω–∞ 3 –¥–Ω—è, –±–µ–∑ –∞–≤—Ç–æ")
totalApartaments += (japanApartaments * 3)
totalPictureTaken += 1

print("–í –∫—Ä–∞–π–Ω–µ–º —Å–ª—É—á–∞–µ —è –Ω–∞–ø—Ä–∞–≤–ª—é—Å—å –≤ \(northKoreaCountry)")
totalTickets += northKoreaTickets
print("–∏ –∫—Ä–∞–π–Ω–∏–π –¥–µ–Ω—å —è –ø—Ä–æ–≤–µ–¥—É –∑–¥–µ—Å—å")
totalApartaments += northKoreaApartaments
var summary = Double(totalTickets) + Double(totalApartaments) + Double(totalVehicleExpanses) + totalOther

print("–ö–∞–∫ –∏—Ç–æ–≥ –≤—Å—ë –ø—É—Ç–µ—à–µ—Å—Ç–≤–∏–µ –º–Ω–µ –æ–±–æ—à–ª–æ—Å—å –≤ \(summary)$ –∏ —è —É—Å–ø–µ—é —Å–¥–µ–ª–∞—Ç—å \(totalPictureTaken) —Ñ–æ—Ç–æ–∫ –º–∏–Ω–∏–º—É–º!!")
