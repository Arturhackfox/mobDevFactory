import UIKit

typealias Emoji = Character
typealias CarModel = (model: String, isAvailable: Bool?, carPrice: Int)
typealias Location = (country: String, tickets: Int, apartaments: Int, carOption: CarModel?, isOnCar: Bool)

// CarPark
let camry:CarModel = ("Toyota Camry", false, 45)
let passat:CarModel = ("Volkswagen Passat", true, 35)
let prius:CarModel = ("Toyota Prius ", true, 50)
let mustang: CarModel? = ("Ford Mustang", true, 85)

// Countries
let netherlandsCountry: Location = ( "Netherlands", 250, 50, camry.isAvailable ?? false ? camry : passat, true)

let franceCountry: Location = ("France", 175, 35, mustang, true)

let newYorkCountry: Location = ("New York", 333, 450, prius, false)

let japanCountry: Location = ("Japan", 250, 50, nil, false)

let northKoreaCountry: Location = ("North Korea", 250, 50, nil, false)

// Emojies
let croissant: Emoji = "ü•ê"
let excitedMood: Emoji = "ü§†"
let taxi: Emoji = "üöï"

//Others
let croissantPrice = 2
let chocolateBar = 2.50
let cola = 4

// Total expanses
var totalTickets = 0
var totalApartaments = 0
var totalVehicleExpanses = 0
var totalPictureTaken = 0
var totalOther: Double = 0
var summary = 0.0


print("–ü—Ä–∏–≤–µ—Ç –¥—Ä—É–∂–∏—â–µ! –°–µ–≥–æ–¥–Ω—è –Ø —Ä–µ—à–∏–ª —É–µ—Ö–∞—Ç—å —Å –ü–æ–ª—å—à–∏ –≤ –¥–æ–ª–≥–æ–∂–¥–∞–Ω–Ω–æ–µ –ø—É—Ç–µ—à–µ—Å—Ç–≤–∏–µ!\(excitedMood)")
print("–ü–æ —Å–ø–∏—Å–∫—É –ø–µ—Ä–≤–æ–π —Å—Ç—Ä–∞–Ω–æ–π –±—É–¥–µ—Ç - \(netherlandsCountry.country)")
print("–î–∞–≤–∞–π —Å–¥–µ–ª–∞–µ–º —Ñ–æ—Ç–æ –º–æ–∏—Ö —á–µ–º–∞–¥–∞–Ω–æ–≤")
totalPictureTaken += 1

print("""
–ø–µ—Ä–µ–ª—ë—Ç –≤ –æ–¥–Ω—É —Å—Ç–æ—Ä–æ–Ω—É –º–Ω–µ –≤—ã–π–¥–µ—Ç \(netherlandsCountry.tickets)$
–∫–∞–∫ –∂–µ –Ω–µ —Å–¥–µ–ª–∞—Ç—å —Ñ–æ—Ç–∫—É –≤ —Å—Ç–æ—Ä–∏—Å –±–∏–ª–µ—Ç–æ–≤ –Ω–∞ —Ñ–æ–Ω–µ –∞–µ—Ä–æ–ø–æ—Ä—Ç–∞
–î–∞–ª–µ–µ –Ø —É–∂–µ –∑–∞—é—Ä–æ–Ω–∏—Ä–æ–≤–∞–ª –æ—Ç–ª–∏—á–Ω—É—é –∏ —É—é–Ω—É—é—Ç –∫–≤–∞—Ä—Ç–∏—Ä–∫—É –Ω–∞ air b&b –Ω–∞ 7 –¥–Ω–µ–π
–ü—Ä–æ—Å—Ç–æ –ø–æ—Å–º–æ—Ç—Ä–∏ –Ω–∞ —ç—Ç–æ—Ç –∫–∞–º–∏–Ω!!!!
–Ø —Å–ª—ã—à–∞–ª, —á—Ç–æ –≤—Å—é –≥–æ–ª–∞–Ω–¥–∏—é –º–æ–∂–Ω–æ –æ–±—å–µ–∑–¥–∏—Ç—å –∑–∞ –Ω–µ–¥–µ–ª—é, –∏ –¥–ª—è —ç—Ç–æ–≥–æ —è —Å–Ω–∏–º—É –ø—Ä–æ—Å—Ç–µ–Ω—å–∫—É—é –º–∞—à–∏–Ω–∫—É \(netherlandsCountry.carOption!.model) –Ω–∞ –Ω–µ–¥–µ–ª—é –∑–∞ \(netherlandsCountry.carOption!.carPrice * 7)$ –Ω–∞ 7 –¥–Ω–µ–π
–£ –Ω–∞—Å –±—É–¥–µ—Ç –≤—Ä–µ–º—è —Å–¥–µ–ª–∞—Ç—å –µ—â–µ –ø–∞—Ä—É —Ñ–æ—Ç–æ—á–µ–∫
""")
totalApartaments += (netherlandsCountry.apartaments * 7)
totalPictureTaken += 1
totalPictureTaken += 1
totalTickets += netherlandsCountry.tickets
totalPictureTaken += 2
totalVehicleExpanses += (netherlandsCountry.carOption!.carPrice * 7)

print("—Å–∞–º–æ–µ –≤—Ä–µ–º—è –ø–æ—Å–µ—Ç–∏—Ç—å –≤—Ç–æ—Ä—É—é –Ω–∞ —Å–ø–∏—Å–∫–µ —Å—Ç—Ä–∞–Ω—É - \(franceCountry.country)")
print("–±–∏–ª–µ—Ç—ã —É–¥–∞–ª–æ—Å—å —Å–ª–æ–≤–∏—Ç—å –Ω–∞ —Å–∫–∏–¥–∫–µ –∑–∞ \(franceCountry.tickets)$ .")
totalTickets += franceCountry.tickets
if franceCountry.isOnCar, let car = franceCountry.carOption {
        print("–Ø –±—É–¥—É –ø—Ä–∏–º–µ—Ä–Ω–æ 2 –¥–Ω—è, –ø–æ—ç—Ç–æ–º—É —Å–Ω—è–ª —É–∫—Ä–æ–º–Ω–æ–µ –º–µ—Å—Ç–µ—á–∫–æ —É –ï–π—Ñ–µ–ª–µ–≤–æ–π –ë–∞—à–Ω–∏ —Å –º–∞—à–∏–Ω–æ–π \(car.model) \(car.model == "Ford Mustang" ? "–±–µ–∑ –∫—Ä—ã—à–∏" : "")")
        
        totalApartaments += franceCountry.apartaments
        totalVehicleExpanses += (car.carPrice * 2)
} else {
    print("–Ø –±—É–¥—É –ø—Ä–∏–º–µ—Ä–Ω–æ 2 –¥–Ω—è, –ø–æ—ç—Ç–æ–º—É —Å–Ω—è–ª —É–∫—Ä–æ–º–Ω–æ–µ –º–µ—Å—Ç–µ—á–∫–æ —É –ï–π—Ñ–µ–ª–µ–≤–æ–π –ë–∞—à–Ω–∏, –¥–µ—Ä–∂–∏ –ø–∞—Ä—É —Ñ–æ—Ç–æ–∫)")
    totalApartaments += franceCountry.apartaments
    totalPictureTaken += 2
}
print("–ó–¥–µ—Å—å –±–µ–∑ –∫—Ä—É–∞—Å–∞–Ω–∞ \(croissant) –Ω–∏–∫–∞–∫, –∫—É–ø–∏–º –∏ –µ–¥—É –≤ –ê–µ—Ä–æ–ø–æ—Ä—Ç")
totalOther += Double(croissantPrice)
totalTickets += newYorkCountry.tickets
totalPictureTaken += 1

print("–ò –≤–æ—Ç –¥–æ–ª–≥–æ–∂–¥–∞–Ω—ã–π \(newYorkCountry.country)!, –∏ –∑–¥–µ—Å—å —è –±–µ—Ä—É —Å–∞–º—É—é –≤–∏–¥–æ–≤—É—é –∫–≤–∞—Ä—Ç–∏—Ä–∫—É –Ω–∞ air b&b –∑–∞ —Ü–µ–ª—ã—Ö \(newYorkCountry.apartaments)$ –≤ —Å—É—Ç–∫–∏!")
print("–í–æ—Å–ø–æ–ª—å–∑—É—é—Å—å –ø–ª–∞—Ç–Ω—ã–º –±–∞—Ä–æ–º –∏ –≤–æ–∑—å–º—É –∫–æ–ª—É —Å —à–æ–∫–æ–ª–∞–¥–∫–æ–π")
print("–ë–ª–∞–≥–æ —è –∑–¥–µ—Å—å –±—É–¥—É 1 –¥–µ–Ω—å, –ø–æ—ç—Ç–æ–º—É –æ—Ç–¥–æ—Ö–Ω—É—Ç—å —Ä–µ—à–∏–ª –Ω–∞ –ø–æ–ª–Ω—É—é!)")
totalApartaments += newYorkCountry.apartaments
totalPictureTaken += 1
totalOther += (chocolateBar + Double(cola))
totalPictureTaken += 1

switch newYorkCountry.isOnCar {
case true:
    print("–ê –≤–æ—Ç –∏ —Ç–∞–∫—Å–∏ \(taxi)!")
    totalVehicleExpanses += 100
case false:
    print("–ó–¥–µ—Å—å —Ç–∞–∫–∏–µ –ø—Ä–æ–±–∫–∏ \(taxi), —á—Ç–æ –ª—É—á—à–µ —è –≤–æ—Å–ø–æ–ª—å–∑—É—é –º–µ—Ç—Ä–æ")
    totalVehicleExpanses += 10
    totalPictureTaken += 1
}

print("–°—Ä–∞–∑—É –ø–æ—Å–ª–µ \(newYorkCountry.country)'a —è —Å—Ä–∞–∑—É –∂–µ –Ω–∞–ø—Ä–∞–≤–ª—é—Å—å –≤ \(japanCountry.country)")
print("–î–∞–π –ë–æ–≥ –º–Ω–µ –æ–¥–æ–±—Ä—è—Ç –≤–∏–∑—É!")
print("–ö–≤–∞—Ä—Ç–∏—Ä—É –≤–æ–∑—å–º—É –Ω–∞ 3 –¥–Ω—è, –±–µ–∑ –∞–≤—Ç–æ")
totalTickets += japanCountry.tickets
totalApartaments += (japanCountry.apartaments * 3)
totalPictureTaken += 1

print("–í –∫—Ä–∞–π–Ω–µ–º —Å–ª—É—á–∞–µ —è –Ω–∞–ø—Ä–∞–≤–ª—é—Å—å –≤ \(northKoreaCountry.country)")
print("–∏ –∫—Ä–∞–π–Ω–∏–π –¥–µ–Ω—å —è –ø—Ä–æ–≤–µ–¥—É –∑–¥–µ—Å—å")
totalTickets += northKoreaCountry.tickets
totalApartaments += northKoreaCountry.apartaments
 summary = Double(totalTickets) + Double(totalApartaments) + Double(totalVehicleExpanses) + totalOther

print("""
      –ö–∞–∫ –∏—Ç–æ–≥ –≤—Å—ë –ø—É—Ç–µ—à–µ—Å—Ç–≤–∏–µ –º–Ω–µ –æ–±–æ—à–ª–æ—Å—å –≤ \(summary)$
      –ù–∞ –±–∏–ª–µ—Ç—ã —É –º–µ–Ω—å —É—à–ª–æ –ø–æ—Ä—è–¥–∫–∞ \(totalTickets)
      –ù–∞ —Å—ä—ë–º –∂–∏–ª—å—è –µ—â—ë + \(totalApartaments)
      –ê –≤–æ—Ç –Ω–∞ –∞—Ä–µ–Ω–¥—É –∞–≤—Ç–æ–º–æ–±–∏–ª–µ–π - \(totalVehicleExpanses)
      –ú–µ–Ω—å—à–µ –≤—Å–µ–≥–æ –∑–∞–Ω—è–ª–∏ —Ä–∞—Å—Å—Ç—Ä–∞—Ç—ã –Ω–∞ —Å–ø–æ–Ω—Ç–∞–Ω–Ω—ã–µ —Å–Ω—ç–∫–∏ - \(totalOther)
      –∏ —è —É—Å–ø–µ–ª —Å–¥–µ–ª–∞—Ç—å \(totalPictureTaken) —Ñ–æ—Ç–æ–∫!!
      """)


